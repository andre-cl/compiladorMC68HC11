**********************************
*DECLARACION DE CONSTANTES
**********************************


**********************************
*DECLARACION DE VARIABLES
**********************************

HH    equ  $0000
HL    equ  $0001
MH    equ  $0002
ML    equ  $0003
SH    equ  $0004
SL    equ  $0005
SEG    equ  $0006
**********************************
*INICIO  DEL PROGRAMA
**********************************
			
 *hola
 


       ORG $8000
INICIO
      CLR HH
      CLR HL
      CLR MH
      CLR ML
      CLR SH
      CLR SL
    CLR SEG
OTRO
   CLR SEG
CICLO
   *LDAA SEG
   *BEQ CICLO

   LDAB SL
   CMPB #9
   BEQ SIGUE
   INC SL
   JMP OTRO

SIGUE
   CLR SL
   LDAB SH
   CMPB #5
   BEQ SIGUE1
   INC SH
   JMP OTRO

SIGUE1
   CLR SH
   CLR SL
   LDAB ML
   CMPB #9
   BEQ SIGUE2
   INC ML
   JMP OTRO

SIGUE2
   CLR ML
   LDAB MH
   CMPB #5
   BEQ SIGUE3
   INC MH
   JMP OTRO

SIGUE3
   CLR MH
   CLR ML
   CLR SH
   CLR SL

   LDAB HH
   CMPB #2
   BEQ SIGUE4
   LDAB HL
   CMPB #9
   BEQ SIGUE5
   INC HL
   JMP OTRO

SIGUE4
   LDAB HL
   CMPB #3   
   BEQ INICIO
   INC HL
   JMP OTRO

SIGUE5
   CLR HL
   INC HH
   JMP OTRO
              
END   